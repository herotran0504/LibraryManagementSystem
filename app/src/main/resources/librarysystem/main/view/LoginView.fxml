<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="193.0"
	prefWidth="370.0"
	styleClass="anchorpane"
	xmlns="http://javafx.com/javafx/17.0.2-ea"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="librarysystem.user.view.LoginView">
	<stylesheets>
		<URL value="@../css/main.css" />
	</stylesheets>
	<children>
		<GridPane hgap="10.0"
			layoutX="62.0"
			layoutY="57.0"
			prefHeight="154.0"
			prefWidth="353.0"
			vgap="10.0">

			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES"
					maxWidth="220.0"
					minWidth="10.0"
					prefWidth="134.0" />
				<ColumnConstraints halignment="CENTER"
					hgrow="ALWAYS"
					maxWidth="333.0"
					minWidth="10.0"
					prefWidth="209.0" />
			</columnConstraints>
			<children>
				<Label contentDisplay="RIGHT"
					prefHeight="17.0"
					prefWidth="94.0"
					text="User Id:"
					GridPane.halignment="RIGHT"
					GridPane.rowIndex="1">
					<font>
						<Font size="13.0" />
					</font>
				</Label>
				<Label prefHeight="17.0"
					prefWidth="94.0"
					text="Password:"
					GridPane.halignment="RIGHT"
					GridPane.rowIndex="2">
					<font>
						<Font size="13.0" />
					</font>
				</Label>
				<TextField fx:id="userId"
					maxHeight="-Infinity"
					maxWidth="-Infinity"
					minWidth="-Infinity"
					prefHeight="25.0"
					prefWidth="207.0"
					text=""
					GridPane.columnIndex="1"
					GridPane.rowIndex="1" />
				<PasswordField fx:id="userPwd"
					maxWidth="-Infinity"
					minWidth="-Infinity"
					onAction="#doLogin"
					prefHeight="25.0"
					prefWidth="206.0"
					text=""
					GridPane.columnIndex="1"
					GridPane.rowIndex="2" />
				<Pane GridPane.columnIndex="1"
					GridPane.halignment="RIGHT"
					GridPane.rowIndex="3">
					<children>
						<Button layoutY="15.0"
							minHeight="36.0"
							minWidth="120.0"
							mnemonicParsing="false"
							onAction="#doLogin"
							prefHeight="36.0"
							prefWidth="120.0"
							text="Submit" />
					</children>
				</Pane>

			</children>
			<rowConstraints>
				<RowConstraints />
				<RowConstraints vgrow="ALWAYS" />
				<RowConstraints />
				<RowConstraints />
			</rowConstraints>
		</GridPane>
		<Label alignment="CENTER"
			contentDisplay="CENTER"
			layoutX="114.0"
			layoutY="20.0"
			prefHeight="48.0"
			prefWidth="320.0"
			text="Login">
			<font>
				<Font name="System Bold"
					size="36.0" />
			</font>
		</Label>
	</children>
</AnchorPane>
